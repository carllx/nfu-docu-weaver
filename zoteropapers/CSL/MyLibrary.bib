@article{2005bhattacharyaDrawingMindCanvas,
  title = {Drawing on Mind's Canvas: Differences in Cortical Integration Patterns between Artists and Non-Artists.},
  author = {Bhattacharya, Joydeep and Petsche, Hellmuth},
  date = {2005-09-01},
  journaltitle = {Human Brain Mapping},
  shortjournal = {Hum. Brain Mapp.},
  volume = {26},
  number = {1},
  pages = {1--14},
  doi = {10.1002/hbm.20104},
  abstract = {Our primary question was to learn whether mentally composing drawings of their own choice produce different brain electric features in artists and laymen. To this purpose, we studied multichannel electroencephalograph (EEG) signals from two broad groups (all participants were females): artists (professionally trained in visual arts) and non-artists (without any training in art). To assess the underlying synchronization, which is assumed to be the platform for general cognitive integration between different cortical regions, three measures inspired by nonlinear dynamical system theory were applied as follows: (1) index based on generalized synchronization; (2) index based on mean phase coherence; and (3) index of phase synchrony based on entropy. Results consistent over all three measures were as follows: comparing the tasks to rest, the artists showed significantly stronger short- and long-range delta band synchronization, whereas the non-artists showed enhancement in short-range beta and gamma band synchronization primarily in frontal regions; comparing the two groups during the tasks, the artists showed significantly stronger delta band synchronization and alpha band desynchronization than did the non-artists. Strong right hemispheric dominance in terms of synchronization was found in the artists. In artists, the higher synchrony in the low-frequency band is possibly due to the involvement of a more advanced long-term visual art memory and to extensive top-down processing. The results demonstrate that in artists, patterns of functional cooperation between cortical regions during mental creation of drawings were significantly different from those in non-artists.},
  langid = {english},
  keywords = {EEG},
  annotation = {102 citations (Crossref) [2023-12-04]\\
JCR分区: Q1\\
中科院分区升级版: 医学2区\\
影响因子: 4.8\\
5年影响因子: 5.4\\
南农高质量: A\\
titleTranslation: 在心灵的画布上绘画：艺术家和非艺术家之间皮质整合模式的差异。\\
abstractTranslation: 我们的首要问题是了解艺术家和外行人在心里创作自己选择的图画是否会产生不同的脑电特征。为此，我们研究了来自两大群体（所有参与者均为女性）的多通道脑电图 (EEG) 信号：艺术家（接受过视觉艺术专业培训）和非艺术家（没有接受过任何艺术培训）。为了评估底层同步，假设底层同步是不同皮层区域之间一般认知整合的平台，受非线性动力系统理论启发，采用了以下三种测量方法：（1）基于广义同步的指标； (2)基于平均相位相干性的指标； (3)基于熵的相位同步指标。所有三项指标的结果一致如下：将任务与休息进行比较，艺术家表现出明显更强的短程和长程 delta 带同步，而非艺术家则主要在额叶表现出短程 beta 和 gamma 带同步的增强地区；在任务期间比较两组，艺术家表现出比非艺术家明显更强的 delta 波段同步和 alpha 波段去同步。艺术家们在同步性方面发现了强烈的右半球优势。对于艺术家来说，低频段的更高同步性可能是由于更先进的长期视觉艺术记忆和广泛的自上而下的处理的参与。结果表明，艺术家在绘画心理创作过程中皮质区域之间的功能合作模式与非艺术家显着不同。\\
remark: "艺术家和非艺术家大脑活动模式不同。"},
  file = {/Users/yamlam/Zotero/storage/8XA7DSQQ/bhattacharya2005.pdf.pdf}
}

@article{2002bhattacharyaShadowsArtistryCortical,
  title = {Shadows of Artistry: Cortical Synchrony during Perception and Imagery of Visual Art.},
  author = {Bhattacharya, Joydeep and Petsche, Hellmuth},
  date = {2002-04-01},
  journaltitle = {Cognitive Brain Research},
  shortjournal = {Cognitive Brain Res.},
  volume = {13},
  number = {2},
  eprint = {11958960},
  eprinttype = {pmid},
  pages = {179--186},
  doi = {10.1016/s0926-6410(01)00110-0},
  abstract = {Functional and topographical differences between two groups, artists and non-artists, during the performances of visual perception and imagery of paintings were presented by means of EEG phase synchrony analysis. In artists as compared with non-artists, significantly higher phase synchrony was found in the high frequency beta and gamma bands during the perception of the paintings; in the low frequency bands (primarily delta), phase synchrony was mostly enhanced during imagery. Strong decreases in phase synchrony of alpha were found primarily in artists for both tasks. The right hemisphere was found to present higher synchrony than the left in artists, whereas hemispheric asymmetry was less significant in non-artists. In the artists, enhanced synchrony in the high frequency band is most likely due to their enhanced binding capabilities of numerous visual attributes, and enhanced synchrony in the low frequency band seems to be due to the higher involvement of long-term visual memory mostly in imagery. Thus, the analysis of phase synchrony from EEG signals yields new information about the dynamical co-operation between neuronal assemblies during the cognition of visual art.},
  langid = {english},
  keywords = {EEG,Vison},
  annotation = {87 citations (Crossref) [2023-12-04]\\
122 citations (Semantic Scholar/DOI) [2023-12-04]\\
MAG ID: 1994059852\\
titleTranslation: 艺术的阴影：视觉艺术感知和意象过程中的皮质同步。\\
abstractTranslation: 通过脑电图相位同步分析，呈现艺术家和非艺术家两组在视觉感知和绘画意象表演过程中的功能和地形差异。与非艺术家相比，在艺术家中，在感知绘画过程中，高频贝塔和伽马频带中发现了明显更高的相位同步性；在低频段（主要是 delta），相位同步在成像过程中大部分得到增强。阿尔法相位同步性的大幅下降主要出现在执行这两项任务的艺术家中。研究发现，艺术家的右半球比左半球表现出更高的同步性，而非艺术家的半球不对称性则不太明显。在艺术家中，高频段同步性的增强很可能是由于他们对众多视觉属性的结合能力增强，而低频段同步性的增强似乎是由于长期视觉记忆（主要是图像）的参与程度更高。因此，对脑电图信号相位同步的分析产生了关于视觉艺术认知过程中神经元组件之间动态合作的新信息。\\
remark: "艺术家与非艺术家在视觉艺术感知中的大脑活动差异。"},
