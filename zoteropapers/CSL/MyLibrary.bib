@article{2000hauflerNeurocognitiveActivitySelfpaced,
  title = {Neuro-Cognitive Activity during a Self-Paced Visuospatial Task: Comparative {{EEG}} Profiles in Marksmen and Novice Shooters},
  shorttitle = {Neuro-Cognitive Activity during a Self-Paced Visuospatial Task},
  author = {Haufler, Amy J. and Spalding, Thomas W. and Santa Maria, D. L. and Hatfield, Bradley D.},
  date = {2000-07-01},
  journaltitle = {Biological Psychology},
