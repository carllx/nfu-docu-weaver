@article{2020liuComprehensiveStudyComparison,
  title = {A Comprehensive Study and Comparison of Core Technologies for {{MPEG}} 3-{{D}} Point Cloud Compression},
  author = {Liu, Hao and Yuan, Hui and Liu, Qi and Hou, Junhui and Liu, Ju},
  date = {2020-09},
  journaltitle = {IEEE Transactions on Broadcasting},
