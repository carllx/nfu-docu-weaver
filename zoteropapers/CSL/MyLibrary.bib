@article{2020gu3DPointCloud,
  title = {{{3D Point Cloud Attribute Compression Using Geometry-Guided Sparse Representation}}},
  author = {Gu, Shuai and Hou, Junhui and Zeng, Huanqiang and Yuan, Hui and Ma, Kai-Kuang},
