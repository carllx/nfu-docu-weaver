  title = {Analysis of {{EEG Signals Related}} to {{Artists}} and {{Nonartists}} during {{Visual Perception}}, {{Mental Imagery}}, and {{Rest Using Approximate Entropy}}},
  author = {Shourie, Nasrin and Firoozabadi, Mohammad and Badie, Kambiz},
  date = {2014},
  journaltitle = {BioMed Research International},
  shortjournal = {BioMed Research International},
