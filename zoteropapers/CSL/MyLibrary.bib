@article{2024HanRenZhiYiZhiZhiShiJingYanDuiChuangZaoXingSiWeiDeYingXiang,
  title = {认知抑制、知识经验对创造性思维的影响},
  author = {韩, 迎春 and 王, 琳萱},
  date = {2024},
  journaltitle = {广东第二师范学院学报},
  shortjournal = {广东第二师范学院学报},
  volume = {44},
  number = {3},
  pages = {98--112},
  issn = {2095-3798},
  url = {https://api6.wenxian.shop/v1/api/download?dflag=pdfdown&v=bmJnH7+KagucGOSli4CxmAym/DC9Rsfc6mbMXvMvQyzv1YoAS4cE94yMVyS9rQqpF/7u+l0jUXI3S32rvcVnZytxwx0Kv7OyRv1XUKSRuCdU3D69cYYgZDtgs9V+RuDqdfgzoYq8tMmsLyu6GU/XZ9hBVNrBCZvTaUrDKL3S8u4lZOZRwWOgeavkqLb3JhU1hMXSPySD/guLo6b5Cpky8MPEN/SG8sNY0V2bS0CwHpM=&fileid=GJXY202403009&dataDbname=CJFQ},
  urldate = {2024-06-20},
  abstract = {认知抑制、知识经验和创造性思维之间的关系，是创造性认知领域的研究热点之一。通过损耗认知资源的方式操纵被试的认知抑制水平，通过项目熟悉度和任务知识情境丰富性评估被试的知识经验水平，以探讨认知抑制与知识经验在创造性思维中的作用机制。研究结果表明：认知抑制在创造性思维的流畅性、灵活性和独特性上主效应均不显著；项目熟悉度在流畅性和灵活性上主效应不显著，在独特性上主效应显著；任务知识情境丰富性在流畅性、灵活性和独特性上主效应均不显著。认知抑制与项目熟悉度在灵活性上交互作用显著；认知抑制与任务知识情境丰富性在灵活性和独特性上交互作用显著；项目熟悉度与任务知识情境丰富性在流畅性和独特性上交互作用显著。研究结论：认知抑制与创造性思维之间的关系总体上符合适应性抑制观点的预测；知识经验与创造性思维之间的关系符合张力观的预测；知识经验对创造性思维的影响受到认知抑制和任务知识情境丰富性的调节。},
  langid = {chinese},
  annotation = {复合影响因子: 0.548\\
综合影响因子: 0.204\\
AMI: 扩展}
}

@article{2024ZhangJuShenRenZhiShiJiaoXiaDeChuangZaoXingSiWei,
  title = {具身认知视角下的创造性思维},
  author = {张, 克 and 杜, 秀敏},
  date = {2024},
  journaltitle = {心理科学进展},
  shortjournal = {心理科学进展},
  pages = {1--12},
  issn = {1671-3710},
  url = {https://api6.wenxian.shop/v1/api/download?dflag=pdfdown&v=bmJnH7+KagucGOSli4CxmAym/DC9Rsfc6mbMXvMvQywMt2sXATbwRBfJXFLKUfdRg1XdNVW/280P4TwwkTgkdu2Dkun/NZfqz3/psCQ/Vw/iG+0pgDgTr6YhnewPOY3y7WetM9s4fL1UssoQQSf757BqSBcaf15PfXELgds54OPiOh+Plox67Sq5/K1Om0oLhMXSPySD/guLo6b5Cpky8MPEN/SG8sNY0V2bS0CwHpM=&fileid=XLXD2024050800C&dataDbname=CAPJ},
  urldate = {2024-06-20},
  abstract = {创造性思维是指产生新颖且适用观点或产品的思维形式。随着具身认知观的兴起,基于该观点的创造性思维研究涌现,但国内相关研究匮乏。为推动国内该领域发展,从身体动作、视觉、触觉、味觉角度对具身认知视角下创造性思维研究进行综述,并从概念隐喻理论、皮亚杰发生认识论、进化心理学说角度对相关具身效应进行解释。未来需从具身效应双向性、认知神经机制、可重复性、多感觉通道、善恶意创造力、机器学习等角度进行深入探索。},
