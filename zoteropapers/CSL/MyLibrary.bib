@article{2021quachDeepPerceptualMetric,
  title = {A Deep Perceptual Metric for {{3D}} Point Clouds},
  author = {Quach, Maurice and Chetouani, Aladine and Valenzise, Giuseppe and Dufaux, Frederic},
  date = {2021-02-25},
  journaltitle = {ArXiv210212839 Cs Eess},
  eprint = {2102.12839},
  eprinttype = {arXiv},
  eprintclass = {cs, eess},
  url = {http://arxiv.org/abs/2102.12839},
  urldate = {2022-01-21},
  abstract = {Point clouds are essential for storage and transmission of 3D content. As they can entail significant volumes of data, point cloud compression is crucial for practical usage. Recently, point cloud geometry compression approaches based on deep neural networks have been explored. In this paper, we evaluate the ability to predict perceptual quality of typical voxel-based loss functions employed to train these networks. We find that the commonly used focal loss and weighted binary cross entropy are poorly correlated with human perception. We thus propose a perceptual loss function for 3D point clouds which outperforms existing loss functions on the ICIP2020 subjective dataset. In addition, we propose a novel truncated distance field voxel grid representation and find that it leads to sparser latent spaces and loss functions that are more correlated with perceived visual quality compared to a binary representation. The source code is available at https://github.com/mauriceqch/2021\_pc\_perceptual\_loss.},
  langid = {english},
  annotation = {5 citations (Semantic Scholar/arXiv) [2023-04-18]\\
titleTranslation: 3D 点云的深度感知度量\\
abstractTranslation: 点云对于 3D 内容的存储和传输至关重要。由于点云压缩可能需要大量数据，因此对于实际使用至关重要。最近，人们探索了基于深度神经网络的点云几何压缩方法。在本文中，我们评估了预测用于训练这些网络的典型基于体素的损失函数的感知质量的能力。我们发现常用的焦点损失和加权二元交叉熵与人类感知的相关性很差。因此，我们提出了一种 3D 点云的感知损失函数，其性能优于 ICIP2020 主观数据集上的现有损失函数。此外，我们提出了一种新颖的截断距离场体素网格表示，并发现与二进制表示相比，它会导致更稀疏的潜在空间和与感知视觉质量更相关的损失函数。源代码可在 https://github.com/mauriceqch/2021\_pc\_perceptual\_loss 获取。},
  file = {/Users/yamlam/Zotero/storage/G2J97HUF/2021_A deep perceptual metric for 3D point clouds.pdf;/Users/yamlam/Zotero/storage/UXVJHRWM/2102.html}
}

