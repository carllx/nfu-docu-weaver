}

@article{2017guNoReferenceQualityMetric,
  title = {No-{{Reference Quality Metric}} of {{Contrast-Distorted Images Based}} on {{Information Maximization}}},
  author = {Gu, Ke and Lin, Weisi and Zhai, Guangtao and Yang, Xiaokang and Zhang, Wenjun and Chen, Chang Wen},
  date = {2017-12},
  journaltitle = {IEEE Transactions on Cybernetics},
  shortjournal = {IEEE Trans. Cybern.},
  volume = {47},
  number = {12},
  pages = {4559--4565},
  issn = {2168-2267, 2168-2275},
  doi = {10.1109/TCYB.2016.2575544},
  url = {http://ieeexplore.ieee.org/document/7492198/},
  urldate = {2023-01-06},
  abstract = {The general purpose of seeing a picture is to attain information as much as possible. With it, we in this paper devise a new no-reference/blind metric for image quality assessment (IQA) of contrast distortion. For local details, we first roughly remove predicted regions in an image since unpredicted remains are of much information. We then compute entropy of particular unpredicted areas of maximum information via visual saliency. From global perspective, we compare the image histogram with the uniformly distributed histogram of maximum information via the symmetric Kullback--Leibler divergence. The proposed blind IQA method generates an overall quality estimation of a contrast-distorted image by properly combining local and global considerations. Thorough experiments on five databases/subsets demonstrate the superiority of our training-free blind technique over state-of-the-art full- and no-reference IQA methods. Furthermore, the proposed model is also applied to amend the performance of general-purpose blind quality metrics to a sizable margin.},
  langid = {english},
  annotation = {230 citations (Semantic Scholar/DOI) [2023-04-18]\\
titleTranslation: 基于信息最大化的对比度失真图像无参考质量度量\\
abstractTranslation: 查看图片的一般目的是获取尽可能多的信息。借助它，我们在本文中设计了一种新的无参考/盲度量，用于对比度失真的图像质量评估（IQA）。对于局部细节，我们首先粗略地删除图像中的预测区域，因为未预测的残留物包含很多信息。然后，我们通过视觉显着性计算最大信息的特定不可预测区域的熵。从全局角度来看，我们通过对称 Kullback-Leibler 散度将图像直方图与最大信息均匀分布直方图进行比较。所提出的盲 IQA 方法通过适当结合局部和全局考虑因素来生成对比度失真图像的整体质量估计。对五个数据库/子集的彻底实验证明了我们的免训练盲技术相对于最先进的全参考和无参考 IQA 方法的优越性。此外，所提出的模型还应用于将通用盲质量指标的性能修正到相当大的幅度。},
  file = {/Users/yamlam/Zotero/storage/LJLPFTMI/2017_No-Reference Quality Metric of Contrast-Distorted Images Based on Information.pdf}
}

@article{2011boscNewQualityMetric,
  title = {Towards a {{New Quality Metric}} for 3-{{D Synthesized View Assessment}}},
  author = {Bosc, Emilie and Pepion, Romuald and Le Callet, Patrick and Koppel, Martin and Ndjiki-Nya, Patrick and Pressigout, Muriel and Morin, Luce},
  date = {2011-11},
  journaltitle = {IEEE Journal of Selected Topics in Signal Processing},
  shortjournal = {IEEE J. Sel. Top. Signal Process.},
  volume = {5},
  number = {7},
  pages = {1332--1343},
  issn = {1932-4553, 1941-0484},
  doi = {10.1109/JSTSP.2011.2166245},
  url = {http://ieeexplore.ieee.org/document/6029277/},
  urldate = {2023-01-06},
  abstract = {3DTV technology has brought out new challenges such as the question of synthesized views evaluation. Synthesized views are generated through a depth image-based rendering (DIBR) process. This process induces new types of artifacts whose impact on visual quality has to be identified considering various contexts of use. While visual quality assessment has been the subject of many studies in the last 20 years, there are still some unanswered questions regarding new technological improvement. DIBR is bringing new challenges mainly because it deals with geometric distortions. This paper considers the DIBR-based synthesized view evaluation problem. Different experiments have been carried out. They question the protocols of subjective assessment and the reliability of the objective quality metrics in the context of 3DTV, in these specific conditions (DIBR-based synthesized views), and they consist in assessing seven different view synthesis algorithms through subjective and objective measurements. Results show that usual metrics are not sufficient for assessing 3-D synthesized views, since they do not correctly render human judgment. Synthesized views contain specific artifacts located around the disoccluded areas, but usual metrics seem to be unable to express the degree of annoyance perceived in the whole image. This study provides hints for a new objective measure. Two approaches are proposed: the first one is based on the analysis of the shifts of the contours of the synthesized view; the second one is based on the computation of a mean SSIM score of the disoccluded areas.},
  langid = {english},
  annotation = {205 citations (Semantic Scholar/DOI) [2023-04-18]\\
titleTranslation: 迈向 3D 合成视图评估的新质量指标\\
abstractTranslation: 3DTV技术带来了新的挑战，例如合成视图评估问题。合成视图是通过基于深度图像的渲染 (DIBR) 过程生成的。此过程会产生新型伪影，必须考虑各种使用环境来识别其对视觉质量的影响。虽然视觉质量评估在过去 20 年中一直是许多研究的主题，但关于新技术改进仍然存在一些未解答的问题。 DIBR 带来了新的挑战，主要是因为它处理几何扭曲。本文考虑基于DIBR的合成视图评估问题。已经进行了不同的实验。他们对 3DTV 背景下的主观评估协议和客观质量指标的可靠性提出质疑，在这些特定条件下（基于 DIBR 的合成视图），他们通过主观和客观测量来评估七种不同的视图合成算法。结果表明，通常的指标不足以评估 3D 合成视图，因为它们不能正确地做出人类判断。合成视图包含位于未遮挡区域周围的特定伪影，但通常的指标似乎无法表达整个图像中感知到的烦恼程度。这项研究为新的客观衡量标准提供了线索。提出了两种方法：第一种方法是基于对合成视图的轮廓移动的分析；第二个是基于去除遮挡区域的平均 SSIM 分数的计算。},
  file = {/Users/yamlam/Zotero/storage/M38G676R/2011_Towards a New Quality Metric for 3-D Synthesized View Assessment.pdf}
}

@article{2020pearceVisualCrossplatformAnalysis,
  title = {Visual Cross-Platform Analysis: Digital Methods to Research Social Media Images},
  shorttitle = {Visual Cross-Platform Analysis},
  author = {Pearce, Warren and \"Ozkula, Suay M. and Greene, Amanda K. and Teeling, Lauren and Bansard, Jennifer S. and Omena, Janna Joceli and Rabello, Elaine Teixeira},
  date = {2020-01-28},
  journaltitle = {Information, Communication \& Society},
  shortjournal = {Inf. Commun. Soc.},
  volume = {23},
  number = {2},
  pages = {161--180},
  publisher = {Routledge},
  issn = {1369-118X},
  doi = {10.1080/1369118X.2018.1486871},
  url = {https://doi.org/10.1080/1369118X.2018.1486871},
  urldate = {2023-01-09},
  abstract = {Analysis of social media using digital methods is a flourishing approach. However, the relatively easy availability of data collected via platform application programming interfaces has arguably led to the predominance of single-platform research of social media. Such research has also privileged the role of text in social media analysis, as a form of data that is more readily gathered and searchable than images. In this paper, we challenge both of these prevailing forms of social media research by outlining a methodology for visual cross-platform analysis (VCPA), defined as the study of still and moving images across two or more social media platforms. Our argument contains three steps. First, we argue that cross-platform analysis addresses a gap in research methods in that it acknowledges the interplay between a social phenomenon under investigation and the medium within which it is being researched, thus illuminating the different affordances and cultures of web platforms. Second, we build on the literature on multimodal communication and platform vernacular to provide a rationale for incorporating the visual into cross-platform analysis. Third, we reflect on an experimental cross-platform analysis of images within social media posts (n = 471,033) used to communicate climate change to advance different modes of macro- and meso-levels of analysis that are natively visual: image-text networks, image plots and composite images. We conclude by assessing the research pathways opened up by VCPA, delineating potential contributions to empirical research and theory and the potential impact on practitioners of social media communication.},
  langid = {english},
  keywords = {climate change,Research methodology,social media,visual analysis},
  annotation = {69 citations (Semantic Scholar/DOI) [2023-04-18]\\
titleTranslation: 视觉跨平台分析：研究社交媒体图像的数字方法\\
abstractTranslation: 使用数字方法分析社交媒体是一种蓬勃发展的方法。然而，通过平台应用程序编程接口收集的数据相对容易获得，可以说导致了社交媒体单一平台研究的主导地位。此类研究还强调了文本在社交媒体分析中的作用，因为它是一种比图像更容易收集和搜索的数据形式。在本文中，我们通过概述视觉跨平台分析（VCPA）的方法来挑战这两种流行的社交媒体研究形式，VCPA被定义为跨两个或多个社交媒体平台的静态和动态图像的研究。我们的论证包含三个步骤。首先，我们认为跨平台分析解决了研究方法中的差距，因为它承认所调查的社会现象与所研究的媒介之间的相互作用，从而阐明网络平台的不同可供性和文化。其次，我们以多模式通信和平台白话的文献为基础，为将视觉纳入跨平台分析提供了基本原理。第三，我们反思了对社交媒体帖子（n = 471,033）中图像的实验性跨平台分析，该分析用于传达气候变化，以推进本地视觉的不同宏观和中观层面的分析模式：图像文本网络，图像图和合成图像。最后，我们评估了 VCPA 开辟的研究路径，描述了对实证研究和理论的潜在贡献以及对社交媒体传播从业者的潜在影响。},
  file = {/Users/yamlam/Zotero/storage/RVY24S9N/2020_Visual cross-platform analysis.pdf}
}

