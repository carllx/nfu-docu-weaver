@article{2019coppensParametricModellingImmersive,
  title = {Parametric {{Modelling Within Immersive Environments}}: {{Building}} a {{Bridge Between Existing Tools}} and {{Virtual Reality Headsets}}},
  shorttitle = {Parametric {{Modelling Within Immersive Environments}}},
  author = {Coppens, Adrien and Mens, Tom and Gallas, Mohamed-Anis},
