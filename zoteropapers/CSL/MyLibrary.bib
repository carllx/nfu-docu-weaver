  title = {Using Chronotope to Research the Space-Time Relations of Learning and Education: {{Dimensions}} of the Unit of Analysis},
  shorttitle = {Using Chronotope to Research the Space-Time Relations of Learning and Education},
  author = {Ritella, Giuseppe and Rajala, Antti and Renshaw, Peter},
  date = {2021-12-01},
