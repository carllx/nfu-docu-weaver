@article{2010desolneuxGestaltTheoryImage,
  title = {From {{Gestalt Theory}} to {{Image Analysis}}: {{A Probabilistic Approach}}},
  author = {Desolneux, Agns and Moisan, Lionel and {Jean-Michel Morel} and Morel, Jean-Michel},
  date = {2010-11-25},
  abstract = {This book introduces the reader to a recent theory in Computer Vision yielding elementary techniques to analyse digital images. These techniques are inspired from and are a mathematical formalization of the Gestalt theory. Gestalt theory, which had never been formalized is a rigorous realm of vision psychology developped between 1923 and 1975. From the mathematical viewpoint the closest field to it is stochastic geometry, involving basic probability and statistics, in the context of image analysis. The authors maintain a public software, MegaWave, containing implementations of most of the image analysis techniques developped in the book. The book is intended for researchers and engineers. It is mathematically self-contained and requires only the basic notions in probability and calculus.},
  langid = {english},
  keywords = {AI,Vison},
  annotation = {MAG ID: 1533452257\\
titleTranslation: 从格式塔理论到图像分析：概率方法\\
abstractTranslation: 本书向读者介绍了计算机视觉的最新理论，产生了分析数字图像的基本技术。这些技术受到格式塔理论的启发，并且是格式塔理论的数学形式化。格式塔理论从未被形式化，是 1923 年至 1975 年间发展起来的视觉心理学的严格领域。从数学角度来看，最接近它的领域是随机几何，涉及图像分析背景下的基本概率和统计。作者维护着一个公共软件 MegaWave，其中包含本书中开发的大多数图像分析技术的实现。本书面向研究人员和工程师。它在数学上是独立的，只需要概率和微积分的基本概念。\\
remark: "基于格式塔理论的计算机视觉图像分析。"}
}

@article{2014bednyThereFutureSensory,
  title = {Is There a Future for Sensory Substitution Outside Academic Laboratories},
  author = {Bedny, Marina and Elli, Giulia V. and Benetti, Stefania and Collignon, Olivier},
  date = {2014-01-01},
  journaltitle = {Multisensory Research},
  shortjournal = {Multisensory Research},
  volume = {27},
  pages = {271--291},
  doi = {10.1163/22134808-00002460},
  abstract = {Sensory substitution devices (SSDs) have been developed with the ultimate purpose of supporting sensory deprived individuals in their daily activities. However, more than forty years after their first appearance in the scientific literature, SSDs still remain more common in research laboratories than in the daily life of people with sensory deprivation. Here, we seek to identify the reasons behind the limited diffusion of SSDs among the blind community by discussing the ergonomic, neurocognitive and psychosocial issues potentially associated with the use of these systems. We stress that these issues should be considered together when developing future devices or improving existing ones. We provide some examples of how to achieve this by adopting a multidisciplinary and participatory approach. These efforts would contribute not solely to address fundamental theoretical research questions, but also to better understand the everyday needs of blind people and eventually promote the use of SSDs outside laboratories.},
  langid = {english},
  keywords = {Creativity,Sensory substitution},
  annotation = {52 citations (Crossref) [2023-12-04]\\
JCR分区: Q4\\
中科院分区升级版: 心理学4区\\
影响因子: 1.6\\
5年影响因子: 1.8\\
EI: 是\\
SSCI: Q4\\
南农高质量: C\\
titleTranslation: 学术实验室之外的感官替代是否有未来\\
abstractTranslation: 开发感觉替代设备 (SSD) 的最终目的是支持感觉缺失个体的日常活动。然而，在科学文献中首次出现四十多年后，SSD 在研究实验室中仍然比在感觉剥夺患者的日常生活中更常见。在这里，我们通过讨论与使用这些系统可能相关的人体工程学、神经认知和社会心理问题，试图找出 SSD 在盲人社区中传播有限的原因。我们强调，在开发未来设备或改进现有设备时，应同时考虑这些问题。我们提供了一些例子，说明如何通过采用多学科和参与性方法来实现这一目标。这些努力不仅有助于解决基础理论研究问题，而且有助于更好地了解盲人的日常需求，并最终促进固态硬盘在实验室之外的使用。\\
remark: "感官替代设备在日常生活中的应用限制。"},
