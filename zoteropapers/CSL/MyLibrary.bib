@inproceedings{2020meynetPCQMFullreferenceQuality,
  title = {{{PCQM}}: {{A}} Full-Reference Quality Metric for Colored {{3D}} Point Clouds},
  shorttitle = {{{PCQM}}},
  booktitle = {2020 {{Twelfth International Conference}} on {{Quality}} of {{Multimedia Experience}} ({{QoMEX}})},
  author = {Meynet, Gabriel and Nehme, Yana and Digne, Julie and Lavoue, Guillaume},
  date = {2020-05},
  pages = {1--6},
  issn = {2472-7814},
