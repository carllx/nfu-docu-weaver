@article{2009boraCognitiveEndophenotypesBipolar,
  title = {Cognitive Endophenotypes of Bipolar Disorder: {{A}} Meta-Analysis of Neuropsychological Deficits in Euthymic Patients and Their First-Degree Relatives},
  shorttitle = {Cognitive Endophenotypes of Bipolar Disorder},
  author = {Bora, Emre and Yucel, Murat and Pantelis, Christos},
