@article{2023raiSemanticEnrichment3D,
  title = {Semantic Enrichment of {{3D}} Point Clouds Using 2d Image Segmentation},
  author = {Rai, A. and Srivastava, N. and Khoshelham, K. and Jain, K.},
  date = {2023-12-14},
  journaltitle = {The International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences},
