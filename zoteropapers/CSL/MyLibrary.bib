@incollection{2010marchAmbiguitiesExperiencea,
  title = {The {{Ambiguities}} of {{Experience}}},
  booktitle = {The {{Ambiguities}} of {{Experience}}},
  author = {March, James G.},
  date = {2010-12-15},
  publisher = {Cornell University Press},
  doi = {10.7591/9780801459016},
  url = {https://www.degruyter.com/document/doi/10.7591/9780801459016/html},
  urldate = {2023-11-28},
  abstract = {"The first component of intelligence involves effective adaptation to an environment. In order to adapt effectively, organizations require resources, capabilities at using them, knowledge about the worlds in which they exist, good fortune, and good decisions. They typically face competition for resources and uncertainties about the future. Many, but possibly not all, of the factors determining their fates are outside their control. Populations of organizations and individual organizations survive, in part, presumably because they possess adaptive intelligence; but survival is by no means assured. The second component of intelligence involves the elegance of interpretations of the experiences of life. Such interpretations encompass both theories of history and philosophies of meaning, but they go beyond such things to comprehend the grubby details of daily existence. Interpretations decorate human existence. They make a claim to significance that is independent of their contribution to effective action. Such intelligence glories in the contemplation, comprehension, and appreciation of life, not just the control of it."---from The Ambiguities of Experience In The Ambiguities of Experience , James G. March asks a deceptively simple question: What is, or should be, the role of experience in creating intelligence, particularly in organizations? Folk wisdom both trumpets the significance of experience and warns of its inadequacies. On one hand, experience is described as the best teacher. On the other hand, experience is described as the teacher of fools, of those unable or unwilling to learn from accumulated knowledge or the teaching of experts. The disagreement between those folk aphorisms reflects profound questions about the human pursuit of intelligence through learning from experience that have long confronted philosophers and social scientists. This book considers the unexpected problems organizations (and the individuals in them) face when they rely on experience to adapt, improve, and survive. While acknowledging the power of learning from experience and the extensive use of experience as a basis for adaptation and for constructing stories and models of history, this book examines the problems with such learning. March argues that although individuals and organizations are eager to derive intelligence from experience, the inferences stemming from that eagerness are often misguided. The problems lie partly in errors in how people think, but even more so in properties of experience that confound learning from it. "Experience," March concludes, "may possibly be the best teacher, but it is not a particularly good teacher."},
  isbn = {978-0-8014-5901-6},
  langid = {english},
  keywords = {AI,Body,experience,experience as a basis for adaptation,experiential learning,human pursuit of intelligence,intelligence,organizational intelligence,organizational lives,power of learning,role of experience},
  annotation = {rate: -1\\
titleTranslation: 经验的模糊性\\
abstractTranslation: ``情报的第一个组成部分涉及对环境的有效适应。为了有效地适应，组织需要资源、使用资源的能力、对其所处世界的了解、好运和良好的决策。他们通常面临资源竞争以及未来的不确定性。决定其命运的许多因素（但可能不是全部）都超出了他们的控制范围。组织群体和个体组织能够生存，部分原因可能是因为它们拥有适应性智能；但生存绝非有保证。智慧的第二个组成部分涉及对生活经历的优雅解释。这种解释既包含历史理论，也包含意义哲学，但它们超越这些东西，去理解日常生活中肮脏的细节。解释装饰了人类的存在。它们使对重要性的主张与他们对有效行动的贡献无关。这种智力的荣耀在于对生活的沉思、理解和欣赏，而不仅仅是对生活的控制。''------摘自《经验的模糊性》 在《经验的模糊性》中，詹姆斯{$\cdot$}G{$\cdot$}马奇提出了一个看似简单的问题：什么是或应该是什么经验在创造智慧方面的作用，特别是在组织中？民间智慧既宣扬经验的重要性，又警告其不足之处。一方面，经验被描述为最好的老师。另一方面，经验被描述为老师这些民间格言之间的分歧反映了人类通过从经验中学习来追求智慧的深刻问题，这些问题长期以来一直困扰着哲学家和社会科学家。组织（以及其中的个人）依靠经验来适应、改进和生存时所面临的意想不到的问题。本书承认从经验中学习的力量以及广泛使用经验作为适应和构建历史故事和模型的基础，同时探讨了这种学习的问题。马奇认为，尽管个人和组织渴望从经验中获取情报，但这种渴望所得出的推论往往会被误导。问题部分在于人们思维方式的错误，但更重要的是在于经验的特性阻碍了从中学习。马奇总结道：``经验可能是最好的老师，但它并不是一个特别好的老师。''\\
remark: "经验在智能构建中的作用及其复杂性。"},
  file = {/Users/yamlam/Zotero/storage/H5X3SZPS/2010_March_The Ambiguities of Experience.pdf}
}

@book{2007minskyEmotionMachineCommonsense,
  title = {The {{Emotion Machine}}: {{Commonsense Thinking}},},
  shorttitle = {The {{Emotion Machine}}},
  author = {Minsky, Marvin},
