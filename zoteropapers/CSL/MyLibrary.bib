@article{2014samiabboudEyeMusicIntroducingVisual,
  title = {{{EyeMusic}}: {{Introducing}} a "Visual" Colorful Experience for the Blind Using Auditory Sensory Substitution.},
  author = {{Sami Abboud} and Abboud, Sami and {Shlomi Hanassy} and Hanassy, Shlomi and Levy-Tzedek, Shelly and Maidenbaum, Shachar and Amedi, Amir},
