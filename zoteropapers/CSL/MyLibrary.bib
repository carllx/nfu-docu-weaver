@article{2019alexiouComprehensiveStudyRatedistortion,
  title = {A Comprehensive Study of the Rate-Distortion Performance in {{MPEG}} Point Cloud Compression},
  author = {Alexiou, Evangelos and Viola, Irene and Borges, Tom\'as M. and Fonseca, Tiago A. and De Queiroz, Ricardo L. and Ebrahimi, Touradj},
