@article{2021qiliuPerceptualQualityAssessment,
  title = {Perceptual {{Quality Assessment}} of {{Colored 3D Point Clouds}}},
  author = {{Qi Liu} and {Honglei Su} and {Zhengfang Duanmu} and {Wentao Liu} and {Zhou Wang}},
  date = {2021},
  journaltitle = {IEEE Transactions on Visualization and Computer Graphics},
  shortjournal = {IEEE Trans. Vis. Comput. Graph.},
  eprint = {35417349},
  eprinttype = {pmid},
  doi = {10.1109/tvcg.2022.3167151},
  abstract = {3D point clouds have found a wide variety of applications in multimedia processing, remote sensing, and scientific computing. Although most point cloud processing systems are developed to improve viewer experiences, little work has been dedicated to perceptual quality assessment of 3D point clouds. In this work, we build a new 3D point cloud database, namely the Waterloo Point Cloud (WPC) database. In contrast to existing datasets consisting of small-scale and low-quality source content of constrained viewing angles, the WPC database contains 20 high quality, realistic, and omni-directional source point clouds and 740 diversely distorted point clouds. We carry out a subjective quality assessment experiment over the database in a controlled lab environment. Our statistical analysis suggests that existing objective point cloud quality assessment (PCQA) models only achieve limited success in predicting subjective quality ratings. We propose a novel objective PCQA model based on an attention mechanism and a variant of information content-weighted structural similarity, which significantly outperforms existing PCQA models. The database has been made publicly available at https://github.com/qdushl/Waterloo-Point-Cloud-Database.},
  langid = {english},
  keywords = {3D Reconstruction,Quality Assessment},
  annotation = {23 citations (Semantic Scholar/DOI) [2023-04-18]\\
ARXIV\_ID: 2111.05474\\
S2ID: 9d04069bb5a761abd86a5c522b3e961f8ea1fb25\\
titleTranslation: 彩色 3D 点云的感知质量评估\\
abstractTranslation: 3D 点云在多媒体处理、遥感和科学计算领域有着广泛的应用。尽管大多数点云处理系统都是为了改善观看者体验而开发的，但很少有人致力于 3D 点云的感知质量评估。在这项工作中，我们构建了一个新的 3D 点云数据库，即 Waterloo Point Cloud (WPC) 数据库。与现有的由小规模、低质量、视角受限的源内容组成的数据集相比，WPC 数据库包含 20 个高质量、逼真、全方位的源点云和 740 个不同扭曲的点云。我们在受控实验室环境中对数据库进行主观质量评估实验。我们的统计分析表明，现有的客观点云质量评估（PCQA）模型在预测主观质量评级方面仅取得有限的成功。我们提出了一种基于注意力机制和信息内容加权结构相似性变体的新颖的客观 PCQA 模型，该模型显着优于现有的 PCQA 模型。该数据库已在 https://github.com/qdushl/Waterloo-Point-Cloud-Database 上公开发布。},
  file = {/Users/yamlam/Zotero/storage/SMBYNIAP/2021_Perceptual Quality Assessment of Colored 3D Point Clouds.pdf}
}

@article{2018linLearningEfficientPoint,
  title = {Learning {{Efficient Point Cloud Generation}} for {{Dense 3D Object Reconstruction}}},
  author = {Lin, Chen-Hsuan and Kong, Chen and Lucey, Simon},
  date = {2018-04-27},
  journaltitle = {Proceedings of the AAAI Conference on Artificial Intelligence},
  shortjournal = {AAAI},
  volume = {32},
  number = {1},
  issn = {2374-3468, 2159-5399},
  doi = {10.1609/aaai.v32i1.12278},
  url = {https://ojs.aaai.org/index.php/AAAI/article/view/12278},
  urldate = {2023-01-06},
  abstract = {Conventional methods of 3D object generative modeling learn volumetric predictions using deep networks with 3D convolutional operations, which are direct analogies to classical 2D ones. However, these methods are computationally wasteful in attempt to predict 3D shapes, where information is rich only on the surfaces. In this paper, we propose a novel 3D generative modeling framework to efficiently generate object shapes in the form of dense point clouds. We use 2D convolutional operations to predict the 3D structure from multiple viewpoints and jointly apply geometric reasoning with 2D projection optimization. We introduce the pseudo-renderer, a differentiable module to approximate the true rendering operation, to synthesize novel depth maps for optimization. Experimental results for single-image 3D object reconstruction tasks show that we outperforms state-of-the-art methods in terms of shape similarity and prediction density.},
  langid = {english},
  keywords = {3D Reconstruction,Semantic Enrichment},
  annotation = {313 citations (Semantic Scholar/DOI) [2023-04-18]\\
titleTranslation: 学习高效点云生成以进行密集 3D 对象重建\\
abstractTranslation: 3D 对象生成建模的传统方法使用具有 3D 卷积运算的深度网络来学习体积预测，这与经典的 2D 方法直接类比。然而，这些方法在尝试预测 3D 形状时在计算上是浪费的，其中信息仅在表面上丰富。在本文中，我们提出了一种新颖的 3D 生成建模框架，可以有效地生成密集点云形式的对象形状。我们使用 2D 卷积运算从多个视点预测 3D 结构，并将几何推理与 2D 投影优化联合应用。我们引入了伪渲染器，这是一个可微分的模块，可以近似真实的渲染操作，以合成新颖的深度图进行优化。单图像 3D 对象重建任务的实验结果表明，我们在形状相似性和预测密度方面优于最先进的方法。},
  file = {/Users/yamlam/Zotero/storage/PQQNSQ5Q/2018_Learning Efficient Point Cloud Generation for Dense 3D Object Reconstruction.pdf}
}

@inproceedings{2021josephProjectStarlineHighfidelity,
  title = {Project {{Starline}}: {{A}} High-Fidelity Telepresence System},
  shorttitle = {Project {{Starline}}},
  author = {Joseph and Desloge, G. and Fortes, Tommy and Gomez, E. M. and H\"aberling, Sascha and Hoppe, Hugues and Andy and Huibers and RICARDOMARTIN-BRUALLA and Andrew and Russell, Ian and Seitz, S. and Tong, Kevin K.},
  date = {2021},
  url = {https://www.semanticscholar.org/paper/Project-Starline%3A-A-high-fidelity-telepresence-Joseph-Desloge/34db8aa914149bba1500c57b2f4633e925a6f6c1},
  urldate = {2023-01-06},
  abstract = {We present a real-time bidirectional communication system that lets two people, separated by distance, experience a face-to-face conversation as if they were copresent. It is the first telepresence system that is demonstrably better than 2D videoconferencing, as measured using participant ratings (e.g., presence, attentiveness, reaction-gauging, engagement), meeting recall, and observed nonverbal behaviors (e.g., head nods, eyebrow movements). This milestone is reached by maximizing audiovisual fidelity and the sense of copresence in all design elements, including physical layout, lighting, face tracking, multi-view capture, microphone array, multi-stream compression, loudspeaker output, and lenticular display. Our system achieves key 3D audiovisual cues (stereopsis, motion parallax, and spatialized audio) and enables the full range of communication cues (eye contact, hand gestures, and body language), yet does not require special glasses or body-worn micro- phones/headphones. The system consists of a head-tracked autostereoscopic display, high-resolution 3D capture and rendering subsystems, and network transmission using compressed color and depth video streams. Other contributions include a novel image-based geometry fusion algorithm, free-space dereverberation, and talker localization.},
  langid = {english},
  keywords = {Compression Techniques,kimi,Semantic Enrichment},
  annotation = {titleTranslation: Project Starline：高保真远程呈现系统\\
abstractTranslation: 我们提出了一种实时双向通信系统，可以让相隔一定距离的两个人体验面对面的对话，就好像他们共处一样。它是第一个明显优于 2D 视频会议的远程呈现系统，通过参与者评分（例如，存在感、注意力、反应测量、参与度）、会议回忆和观察到的非语言行为（例如，点头、眉毛移动）来衡量。这一里程碑是通过最大限度地提高所有设计元素的视听保真度和共存感来实现的，包括物理布局、照明、面部跟踪、多视图捕捉、麦克风阵列、多流压缩、扬声器输出和光栅显示。我们的系统实现了关键的 3D 视听线索（立体视觉、运动视差和空间化音频），并实现了全方位的沟通线索（眼神接触、手势和肢体语言），但不需要特殊的眼镜或随身佩戴的麦克风/耳机。该系统由头部跟踪自动立体显示器、高分辨率 3D 捕获和渲染子系统以及使用压缩彩色和深度视频流的网络传输组成。其他贡献包括新颖的基于图像的几何融合算法、自由空间混响和说话者定位。\\
