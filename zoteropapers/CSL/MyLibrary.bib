@article{2021funkeFivePointsCheck,
  title = {Five Points to Check When Comparing Visual Perception in Humans and Machines},
  author = {Funke, Christina M. and Borowski, Judy and Stosio, Karolina and Brendel, Wieland and Wallis, Thomas S. A. and Bethge, Matthias},
  date = {2021-03-16},
  journaltitle = {Journal of Vision},
  shortjournal = {J. Vis.},
  volume = {21},
  number = {3},
  pages = {16},
  issn = {1534-7362},
  doi = {10.1167/jov.21.3.16},
  url = {https://jov.arvojournals.org/article.aspx?articleid=2772393},
  urldate = {2023-11-24},
  abstract = {With the rise of machines to human-level performance in complex recognition tasks, a growing amount of work is directed toward comparing information processing in humans and machines. These studies are an exciting chance to learn about one system by studying the other. Here, we propose ideas on how to design, conduct, and interpret experiments such that they adequately support the investigation of mechanisms when comparing human and machine perception. We demonstrate and apply these ideas through three case studies. The first case study shows how human bias can affect the interpretation of results and that several analytic tools can help to overcome this human reference point. In the second case study, we highlight the difference between necessary and sufficient mechanisms in visual reasoning tasks. Thereby, we show that contrary to previous suggestions, feedback mechanisms might not be necessary for the tasks in question. The third case study highlights the importance of aligning experimental conditions. We find that a previously observed difference in object recognition does not hold when adapting the experiment to make conditions more equitable between humans and machines. In presenting a checklist for comparative studies of visual reasoning in humans and machines, we hope to highlight how to overcome potential pitfalls in design and inference.},
  langid = {english},
  keywords = {HCI,Vison},
  annotation = {32 citations (Crossref) [2023-12-04]\\
JCR分区: Q3\\
中科院分区升级版: 医学4区\\
影响因子: 1.8\\
5年影响因子: 1.9\\
南农高质量: B\\
titleTranslation: 比较人类和机器的视觉感知时需要检查的五个要点\\
abstractTranslation: 随着机器在复杂识别任务中的性能提升到人类水平，越来越多的工作转向比较人类和机器的信息处理。这些研究是通过研究一个系统来了解另一个系统的令人兴奋的机会。在这里，我们提出了如何设计、进行和解释实验的想法，以便在比较人类和机器感知时充分支持机制的研究。我们通过三个案例研究展示并应用这些想法。第一个案例研究展示了人类偏见如何影响结果的解释，以及几种分析工具可以帮助克服这个人类参考点。在第二个案例研究中，我们强调了视觉推理任务中必要机制和充分机制之间的区别。因此，我们表明，与之前的建议相反，反馈机制对于相关任务可能不是必需的。第三个案例研究强调了调整实验条件的重要性。我们发现，当调整实验以使人类和机器之间的条件更加公平时，先前观察到的物体识别差异并不成立。在提供人类和机器视觉推理比较研究清单时，我们希望强调如何克服设计和推理中的潜在陷阱。\\
remark: "对比人类和机器视觉感知的研究要点。"},
  file = {/Users/yamlam/Zotero/storage/Z393RKS5/2021_Funke et al_Five points to check when comparing visual perception in humans and machines.pdf}
}

@article{2022liuSelfCueingRealTimeAttention,
  title = {Self-{{Cueing Real-Time Attention Scheduling}} in {{Criticality-Aware Visual Machine Perception}}},
  author = {Liu, Shengzhong and Fu, Xinzhe and Wigness, Maggie and David, Philip and Yao, Shuochao and Sha, Lui and Abdelzaher, Tarek},
  date = {2022-05},
  journaltitle = {2022 IEEE 28th Real-Time and Embedded Technology and Applications Symposium (RTAS)},
  shortjournal = {2022 IEEE 28th Real-Time and Embedded Technology and Applications Symposium (RTAS)},
  pages = {173--186},
  publisher = {IEEE},
  location = {Milano, Italy},
  doi = {10.1109/RTAS54340.2022.00022},
  url = {https://ieeexplore.ieee.org/document/9804626/},
  urldate = {2023-11-24},
  abstract = {This paper presents a self-cueing real-time frame-work for attention prioritization in AI-enabled visual perception systems that minimizes a notion of state uncertainty. By attention prioritization we refer to inspecting some parts of the scene before others in a criticality-aware fashion. By self-cueing, we refer to not needing external cueing sensors for prioritizing attention, thereby simplifying design. We show that attention prioritization saves resources, thus enabling more efficient and responsive real-time object tracking on resource-limited embedded platforms. The system consists of two components: First, an optical flow-based module decides on the regions to be viewed on a subframe level, as well as their criticality. Second, a novel batched proportional balancing (BPB) scheduling policy decides how to schedule these regions for inspection by a deep neural network (DNN), and how to parallelize execution on the GPU. We implement the system on an NVIDIA Jetson Xavier platform, and empirically demonstrate the superiority of the proposed architecture through an extensive evaluation using a real-word driving dataset.},
  eventtitle = {2022 {{IEEE}} 28th {{Real-Time}} and {{Embedded Technology}} and {{Applications Symposium}} ({{RTAS}})},
  isbn = {9781665499989},
