@article{2020graziosiOverviewOngoingPoint,
  title = {An Overview of Ongoing Point Cloud Compression Standardization Activities: Video-Based ({{V-PCC}}) and Geometry-Based ({{G-PCC}})},
  shorttitle = {An Overview of Ongoing Point Cloud Compression Standardization Activities},
  author = {Graziosi, Danillo B. and Nakagami, O. and Kuma, S. and Zaghetto, Alexandre and Suzuki, Teruhiko and Tabatabai, Ali},
  date = {2020-01-01},
  journaltitle = {APSIPA Transactions on Signal and Information Processing},
  shortjournal = {APSIPA Transactions on Signal and Information Processing},
  volume = {9},
  number = {1},
  issn = {2048-7703},
  doi = {10.1017/atsip.2020.12},
  url = {https://www.cambridge.org/core/journals/apsipa-transactions-on-signal-and-information-processing/article/an-overview-of-ongoing-point-cloud-compression-standardization-activities-videobased-vpcc-and-geometrybased-gpcc/56FCAF660DD44348BCB1BCA9B5EC56CF},
  abstract = {This article presents an overview of the recent standardization activities for point cloud compression (PCC). A point cloud is a 3D data representation used in diverse applications associated with immersive media including virtual/augmented reality, immersive telepresence, autonomous driving and cultural heritage archival. The international standard body for media compression, also known as the Motion Picture Experts Group (MPEG), is planning to release in 2020 two PCC standard specifications: video-based PCC (V-CC) and geometry-based PCC (G-PCC). V-PCC and G-PCC will be part of the ISO/IEC 23090 series on the coded representation of immersive media content. In this paper, we provide a detailed description of both codec algorithms and their coding performances. Moreover, we will also discuss certain unique aspects of point cloud compression.},
  langid = {english},
  keywords = {3D Reconstruction,Compression Techniques,G-PCC,Geometry-based point cloud compression,Immersive media,ISO/IEC 23090-5,ISO/IEC 23090-9,kimi,MPEG-I,Point cloud compression,V-PCC,Video-based point cloud compression},
  annotation = {117 citations (Semantic Scholar/DOI) [2023-04-18]: undefined\\
MAG ID: 3014200484\\
S2ID: 50e133c082b424d7d3a09587d86c3fe9d5d59004\\
titleTranslation: 正在进行的点云压缩标准化活动概述：基于视频 (V-PCC) 和基于几何 (G-PCC)\\
abstractTranslation: 本文概述了点云压缩 (PCC) 的最新标准化活动。点云是一种 3D 数据表示，用于与沉浸式媒体相关的各种应用，包括虚拟/增强现实、沉浸式远程呈现、自动驾驶和文化遗产档案。媒体压缩国际标准机构，也称为运动图像专家组（MPEG），计划在2020年发布两个PCC标准规范：基于视频的PCC（V-CC）和基于几何的PCC（G-PCC） 。 V-PCC 和 G-PCC 将成为有关沉浸式媒体内容编码表示的 ISO/IEC 23090 系列的一部分。在本文中，我们详细描述了两种编解码器算法及其编码性能。此外，我们还将讨论点云压缩的某些独特方面。\\
