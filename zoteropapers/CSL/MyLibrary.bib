}

@book{2010macphersonSensesClassicContemporary,
  title = {The {{Senses}}: {{Classic}} and {{Contemporary Philosophical Perspectives}}},
  shorttitle = {The {{Senses}}},
  author = {Macpherson, Fiona},
  date = {2010-11-23},
  eprint = {wa1oAgAAQBAJ},
  eprinttype = {googlebooks},
  publisher = {Oxford University Press},
  abstract = {The senses, or sensory modalities, constitute the different ways we have of perceiving the world, such as seeing, hearing, touching, tasting and smelling. But how many senses are there? How many could there be? What makes the senses different? What interaction takes place between the senses? This book is a guide to thinking about these questions. Together with an extensive introduction to the topic, the book contains the key classic papers on this subject together with nine newly commissioned essays. One reason that these questions are important is that we are receiving a huge influx of new information from the sciences that challenges some traditional philosophical views about the senses. This information needs to be incorporated into our view of the senses and perception. Can we do this whilst retaining our pre-existing concepts of the senses and of perception or do we need to revise our concepts? If they need to be revised, then in what way should that be done? Research in diverse areas, such as the nature of human perception, varieties of non-human animal perception, the interaction between different sensory modalities, perceptual disorders, and possible treatments for them, calls into question the platitude that there are five senses, as well as the pre-supposition that we know what we are counting when we count them as five (or more). This book will serve as an inspiring introduction to the topic and as a basis from which further new research will grow.},
  isbn = {978-0-19-978072-3},
  langid = {english},
  pagetotal = {425},
  keywords = {Philosophy,Philosophy / General,Philosophy / Mind & Body},
  annotation = {titleTranslation: 感官：经典与当代哲学视角\\
abstractTranslation: 感官或感觉方式构成了我们感知世界的不同方式，例如看、听、触摸、品尝和嗅觉。但有多少种感官呢？可能有多少个？是什么让感官有所不同？感官之间发生什么相互作用？本书是思考这些问题的指南。除了对该主题的广泛介绍之外，本书还包含有关该主题的重要经典论文以及九篇新委托的论文。这些问题之所以重要，原因之一是我们正在从科学中接收到大量新信息，这些信息挑战了一些关于感官的传统哲学观点。这些信息需要纳入我们的感官和感知视图中。我们可以在保留我们预先存在的感官和知觉概念的同时做到这一点，还是需要修改我们的概念？如果需要修改的话，应该怎样修改呢？不同领域的研究，例如人类感知的本质、非人类动物感知的多样性、不同感觉方式之间的相互作用、知觉障碍及其可能的治疗方法，对有五种感觉的陈词滥调提出了质疑作为一个前提假设，当我们数到五个（或更多）时，我们知道我们在数什么。本书将作为对该主题的鼓舞人心的介绍，并作为进一步新研究发展的基础。\\
remark: "探讨感官知觉的多样性和复杂性。"},
  file = {/Users/yamlam/Zotero/storage/ZZ9H9GXY/2010_Macpherson_The Senses.pdf}
}

@article{2005raymondEffectsAlphaTheta,
  title = {The Effects of Alpha/Theta Neurofeedback on Personality and Mood},
  author = {Raymond, Joshua and Varney, Carolyn and Parkinson, Lesley A. and Gruzelier, John},
  date = {2005-05-01},
  journaltitle = {Cognitive Brain Research},
