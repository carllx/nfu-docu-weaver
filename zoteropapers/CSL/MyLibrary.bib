@article{1998witmerMeasuringPresenceVirtual,
  title = {Measuring {{Presence}} in {{Virtual Environments}}: {{A Presence Questionnaire}}},
  author = {Witmer, Bob G. and {Michael J. Singer} and Singer, Michael J.},
  date = {1998-06-01},
  journaltitle = {Teleoperators and Virtual Environments},
  shortjournal = {Teleoperators Virtual Environ.},
  volume = {7},
  number = {3},
  pages = {225--240},
  doi = {10.1162/105474698565686},
  abstract = {The effectiveness of virtual environments (VEs) has often been linked to the sense of presence reported by users of those VEs. (Presence is defined as the subjective experience of being in one place or environment, even when one is physically situated in another.) We believe that presence is a normal awareness phenomenon that requires directed attention and is based in the interaction between sensory stimulation, environmental factors that encourage involvement and enable immersion, and internal tendencies to become involved. Factors believed to underlie presence were described in the premier issue of Presence: Teleoperators and Virtual Environments. We used these factors and others as the basis for a presence questionnaire (PQ) to measure presence in VEs. In addition we developed an immersive tendencies questionnaire (ITQ) to measure differences in the tendencies of individuals to experience presence. These questionnaires are being used to evaluate relationships among reported presence and other research variables. Combined results from four experiments lead to the following conclusions:  the PQ and ITQ are internally consistent measures with high reliability; there is a weak but consistent positive relation between presence and task performance in VEs; individual tendencies as measured by the ITQ predict presence as measured by the PQ; and individuals who report more simulator sickness symptoms in VE report less presence than those who report fewer symptoms.},
  langid = {english},
  keywords = {Compression Techniques,Psychology,User Interaction},
  annotation = {5006 citations (Semantic Scholar/DOI) [2023-04-18]\\
MAG ID: 2136757566\\
titleTranslation: 衡量虚拟环境中的存在感：存在感问卷\\
abstractTranslation: 虚拟环境 (VE) 的有效性通常与这些 VE 用户报告的存在感相关。 （存在感被定义为在一个地方或环境中的主观体验，即使一个人实际上位于另一个地方。）我们认为，存在感是一种正常的意识现象，需要直接关注，并且基于感官刺激、环境刺激之间的相互作用。鼓励参与和沉浸的因素，以及参与的内部倾向。 《存在：远程操作员和虚拟环境》第一期中描述了被认为构成存在的因素。我们使用这些因素和其他因素作为存在问卷 (PQ) 的基础来衡量 VE 中的存在。此外，我们还开发了沉浸式倾向问卷（ITQ）来衡量个体体验临在感的倾向差异。这些调查问卷用于评估报告的存在与其他研究变量之间的关系。综合四个实验的结果得出以下结论：PQ和ITQ是内部一致的度量，具有高可靠性； VE 中的存在感和任务绩效之间存在微弱但一致的正相关关系；由 ITQ 衡量的个人倾向预测由 PQ 衡量的存在；在 VE 中报告更多模拟晕眩症状的人比报告症状较少的人报告的存在更少。},
  file = {/Users/yamlam/Zotero/storage/DCXVJABM/1998_Measuring Presence in Virtual Environments.pdf}
}

@article{1994slaterDepthPresenceVirtual,
  title = {Depth of Presence in Virtual Environments},
  author = {Slater, Mel and Usoh, Martin and Steed, Anthony},
  date = {1994-01-01},
  journaltitle = {Presence: Teleoperators \& Virtual Environments},
  shortjournal = {Presence Teleoperators Virtual Environ.},
  volume = {3},
  number = {2},
  pages = {130--144},
  doi = {10.1162/pres.1994.3.2.130},
  abstract = {This paper describes a study to assess the influence of a variety of factors on reported level of presence in immersive virtual environments. It introduces the idea of ``stacking depth,'' that is, where a participant can simulate the process of entering the virtual environment while already in such an environment, which can be repeated to several levels of depth. An experimental study including 24 subjects was carried out. Half of the subjects were transported between environments by using virtual head-mounted displays, and the other half by going through doors. Three other binary factors were whether or not gravity operated, whether or not the subject experienced a virtual precipice, and whether or not the subject was followed around by a virtual actor. Visual, auditory, and kinesthetic representation systems and egocentric/exocentric perceptual positions were assessed by a preexperiment questionnaire. Presence was assessed by the subjects as their sense of ``being there,'' the extent to which they experienced the virtual environments as more the presenting reality than the real world in which the experiment was taking place, and the extent to which the subject experienced the virtual environments as places visited rather than images seen. A logistic regression analysis revealed that subjective reporting of presence was significantly positively associated with visual and kinesthetic representation systems, and negatively with the auditory system. This was not surprising since the virtual reality system used was primarily visual. The analysis also showed a significant and positive association with stacking level depth for those who were transported between environments by using the virtual HMD, and a negative association for those who were transported through doors. Finally, four of the subjects moved their real left arm to match movement of the left arm of the virtual body displayed by the system. These four scored significantly higher on the kinesthetic representation system than the remainder of the subjects.},
  langid = {english},
  annotation = {1175 citations (Semantic Scholar/DOI) [2023-04-18]\\
MAG ID: 1586856071\\
titleTranslation: 虚拟环境中的存在深度\\
abstractTranslation: 本文介绍了一项研究，旨在评估各种因素对沉浸式虚拟环境中报告的存在水平的影响。它引入了``堆叠深度''的概念，即参与者可以在已经处于虚拟环境中的情况下模拟进入虚拟环境的过程，并且可以重复到多个深度级别。进行了一项包括 24 名受试者的实验研究。一半的受试者通过使用虚拟头戴式显示器在环境之间进行传输，另一半则通过门进行传输。另外三个二元因素是重力是否起作用、受试者是否经历了虚拟悬崖以及受试者是否被虚拟演员跟踪。通过实验前问卷评估视觉、听觉和动觉表征系统以及自我中心/外中心感知位置。受试者对存在感的评估是他们``身临其境''的感觉，他们在多大程度上体验到虚拟环境比实验进行的现实世界更接近现实世界的程度，以及受试者体验到虚拟环境的程度。虚拟环境是参观过的地方，而不是看到的图像。逻辑回归分析显示，存在的主观报告与视觉和动觉表征系统呈显着正相关，与听觉系统呈负相关。这并不奇怪，因为所使用的虚拟现实系统主要是视觉的。分析还显示，对于使用虚拟 HMD 在环境之间运输的人来说，堆垛深度与堆叠深度存在显着的正相关关系，而对于通过门运输的人来说，则与堆叠水平深度存在负相关关系。最后，四名受试者移动他们真实的左臂，以匹配系统显示的虚拟身体的左臂的移动。这四人在动觉表征系统上的得分明显高于其余受试者。},
  file = {/Users/yamlam/Zotero/storage/CGLSRIWE/1994_Depth of presence in virtual environments.pdf}
}

