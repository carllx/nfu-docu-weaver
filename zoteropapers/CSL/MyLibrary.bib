@article{2014SunRiChangShengHuoShiYeZhongDeLuYouSheQuKongJianZaiShengChanYanJiuJiYuLieFeiFuErYuDeSaiTuDeLiLunShiJiao,
  title = {日常生活视野中的旅游社区空间再生产研究------基于列斐伏尔与德塞图的理论视角},
