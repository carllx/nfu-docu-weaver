@article{2015seufertSurveyQualityExperience,
  title = {A {{Survey}} on {{Quality}} of {{Experience}} of {{HTTP Adaptive Streaming}}},
  author = {Seufert, Michael and Egger, Sebastian and Slanina, Martin and Zinner, Thomas and Hobfeld, Tobias and Tran-Gia, Phuoc},
  date = {2015-01-21},
  journaltitle = {IEEE Communications Surveys and Tutorials},
  shortjournal = {IEEE Commun. Surv. Tutor.},
  volume = {17},
  number = {1},
  pages = {469--492},
  doi = {10.1109/comst.2014.2360940},
  abstract = {Changing network conditions pose severe problems to video streaming in the Internet. HTTP adaptive streaming (HAS) is a technology employed by numerous video services that relieves these issues by adapting the video to the current network conditions. It enables service providers to improve resource utilization and Quality of Experience (QoE) by incorporating information from different layers in order to deliver and adapt a video in its best possible quality. Thereby, it allows taking into account end user device capabilities, available video quality levels, current network conditions, and current server load. For end users, the major benefits of HAS compared to classical HTTP video streaming are reduced interruptions of the video playback and higher bandwidth utilization, which both generally result in a higher QoE. Adaptation is possible by changing the frame rate, resolution, or quantization of the video, which can be done with various adaptation strategies and related client- and server-side actions. The technical development of HAS, existing open standardized solutions, but also proprietary solutions are reviewed in this paper as fundamental to derive the QoE influence factors that emerge as a result of adaptation. The main contribution is a comprehensive survey of QoE related works from human computer interaction and networking domains, which are structured according to the QoE impact of video adaptation. To be more precise, subjective studies that cover QoE aspects of adaptation dimensions and strategies are revisited. As a result, QoE influence factors of HAS and corresponding QoE models are identified, but also open issues and conflicting results are discussed. Furthermore, technical influence factors, which are often ignored in the context of HAS, affect perceptual QoE influence factors and are consequently analyzed. This survey gives the reader an overview of the current state of the art and recent developments. At the same time, it targets networking researchers who develop new solutions for HTTP video streaming or assess video streaming from a user centric point of view. Therefore, this paper is a major step toward truly improving HAS.},
  langid = {english},
  keywords = {Semantic Enrichment,User Interaction},
  annotation = {746 citations (Semantic Scholar/DOI) [2023-04-18]\\
MAG ID: 1986570368\\
titleTranslation: HTTP 自适应流媒体体验质量调查\\
abstractTranslation: 不断变化的网络条件给互联网上的视频流带来了严重的问题。 HTTP 自适应流式传输 (HAS) 是一种被众多视频服务采用的技术，它通过使视频适应当前的网络条件来缓解这些问题。它使服务提供商能够通过整合来自不同层的信息来提高资源利用率和体验质量 (QoE)，以便以最佳质量交付和调整视频。因此，它允许考虑最终用户设备功能、可用视频质量级别、当前网络状况和当前服务器负载。对于最终用户而言，与传统 HTTP 视频流相比，HAS 的主要优势是减少视频播放中断和提高带宽利用率，这两者通常都会带来更高的 QoE。通过改变视频的帧速率、分辨率或量化，可以进行适应，这可以通过各种适应策略以及相关的客户端和服务器端操作来完成。本文回顾了 HAS 的技术发展、现有的开放标准化解决方案以及专有解决方案，将其作为导出因适应而出现的 QoE 影响因素的基础。主要贡献是对人机交互和网络领域的 QoE 相关工作进行了全面的调查，这些工作是根据视频适应的 QoE 影响而构建的。更准确地说，我们重新审视了涵盖适应维度和策略的 QoE 方面的主观研究。结果，确定了 HAS 的 QoE 影响因素和相应的 QoE 模型，同时还讨论了悬而未决的问题和相互矛盾的结果。此外，在 HAS 背景下经常被忽视的技术影响因素也会影响感知 QoE 影响因素，因此需要进行分析。这项调查向读者提供了当前技术水平和最新发展的概述。同时，它还针对为 HTTP 视频流开发新解决方案或从以用户为中心的角度评估视频流的网络研究人员。因此，本文是朝着真正改进 HAS 迈出的重要一步。},
  file = {/Users/yamlam/Zotero/storage/TGT6HUUP/2015_A Survey on Quality of Experience of HTTP Adaptive Streaming.pdf}
}

@article{2006schnabelOctreebasedPointcloudCompression,
  title = {Octree-Based Point-Cloud Compression},
  author = {Schnabel, Ruwen and Klein, Reinhard},
  date = {2006-07-29},
  pages = {111--121},
  doi = {10.2312/spbg/spbg06/111-120},
  abstract = {In this paper we present a progressive compression method for point sampled models that is specifically apt at dealing with densely sampled surface geometry. The compression is lossless and therefore is also suitable for storing the unfiltered, raw scan data. Our method is based on an octree decomposition of space. The point-cloud is encoded in terms of occupied octree-cells. To compress the octree we employ novel prediction techniques that were specifically designed for point sampled geometry and are based on local surface approximations to achieve high compression rates that outperform previous progressive coders for point-sampled geometry. Moreover we demonstrate that additional point attributes, such as color, which are of great importance for point-sampled geometry, can be well integrated and efficiently encoded in this framework.},
  langid = {english},
  keywords = {Compression Techniques,User Interaction},
  annotation = {384 citations (Semantic Scholar/DOI) [2023-04-18]\\
MAG ID: 1529450504\\
titleTranslation: 基于八叉树的点云压缩\\
abstractTranslation: 在本文中，我们提出了一种针对点采样模型的渐进压缩方法，该方法特别适合处理密集采样的表面几何形状。压缩是无损的，因此也适合存储未过滤的原始扫描数据。我们的方法基于空间的八叉树分解。点云按照占用的八叉树单元进行编码。为了压缩八叉树，我们采用了专门为点采样几何设计的新颖预测技术，并且基于局部表面近似来实现高压缩率，该压缩率优于以前的点采样几何渐进编码器。此外，我们证明了对于点采样几何非常重要的其他点属性（例如颜色）可以在该框架中很好地集成和有效编码。}
}

